{
  "expo": {
    "scheme": "threec",
    "platforms": [
      "ios",
      "android"
    ],
    "name": "3c",
    "slug": "3c",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/icon/ios-adaptive-light-icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "notification": {
      "icon": "./assets/icon/notification.png",
      "color": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.c3school.app",
      "buildNumber": "5",
      "icon": {
        "dark": "./assets/icon/ios-adaptive-dark-icon.png",
        "light": "./assets/icon/ios-adaptive-light-icon.png"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/icon/android-adaptive-icon.png"
      },
      "permissions": [
        "NOTIFICATIONS"
      ],
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "app.c3school.current",
      "versionCode": 5
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icon/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon/notification.png",
          "color": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9fa16be6-9e25-49af-b344-9dc9b15f376e"
      },
      "apiUrl": "https://3cschool.net/api"
    },
    "owner": "graphicode"
  }
}
